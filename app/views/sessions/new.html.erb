<h1> Login Page </h1> 

    <%= flash[:error] %>
    <%= flash[:notice] %>

    <%= form_for(:session, url: login_path) do |f|%>
        
        <%= f.label :username %> 
        <%= f.text_field :username %> 

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Log In" %>
    <% end %>

<br> <br> 
<h2> <%= link_to "Sign Up", signup_path %> </h2>

<%= link_to "Sign In With Google", "auth/google_oauth2" %>