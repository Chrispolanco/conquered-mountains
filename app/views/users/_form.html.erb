<h3> New User Form </h3> 

    <%= form_for @user do |f| %> 
    
    <%= f.label 'username' %><br>
    <%= f.text_field :username %><br> 

    <%= f.label 'email' %><br>
    <%= f.text_field :email %><br> 

    <%= f.label 'name' %><br>
    <%= f.text_field :name %><br> 

    <%= f.submit "Submit Climber" %>

<% end %> 