<h3> New User Form </h3> 

<%= render partial: 'layouts/errors', locals: {object: @user}%>

    <%= form_for @user do |f| %> 

    <div class="field">
        <%= f.label 'username' %><br>
        <%= f.text_field :username %><br> 
    </div> 

    <div class="field">
        <%= f.label 'email' %><br>
        <%= f.text_field :email %><br> 
    </div> 

    <div class="field">
        <%= f.label 'name' %><br>
        <%= f.text_field :name %><br> 
    </div> 

    <div class="field">
        <%= f.label 'password' %><br>
        <%= f.password_field :password %><br> 
    </div>  

    <div class="actions">
        <%= f.submit "Submit Climber" %>
    </div> 

<% end %> 


    